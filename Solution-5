#include <bits/stdc++.h>
using namespace std;
#define fori(i,a,b) for (int i=a;i<b;i++)
int main() {
    ios::sync_with_stdio(0);
    cin.tie(0);
	string str;
	while (cin>>str){
	    int x=0,y=0,flag=0;
	    int res;
	    
	    fori(a,1,11){
	        if (str[2*a-2]=='1') x++;
	        if (a<6){
	            int m=5-a;
	            int o=abs(x-y);
	            if (o>m+1 && x>y) {
	                flag=1;
	                res=2*a-1;
	            }
	            else if (o>m && y>x){
	                flag=2;
	                res=2*a-1;
	            }
	        }
	        if (flag!=0) break;
	        if (str[2*a-1]=='1') y++;
	        if (a>5){
	            if (x>y) {
	                flag=1;
	                res=2*a;
	            }
	            else if (y>x){ flag=2; res=2*a;}
	        }
	        else {
	            int m=5-a;
	            int o=abs(x-y);
	            if (o>m){
	                if (y>x) flag=2;
	                else flag=1;
	                res=2*a;
	            }
	            
	        }
	        if (flag!=0) break;
	        
	    }
	    if (flag==1) cout<<"TEAM-A "<<res;
	    else if (flag==2) cout<<"TEAM-B "<<res;
	    else cout<<"TIE ";
	    cout<<"\n";
	}
	return 0;
}
